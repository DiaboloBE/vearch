include "status.fbs";

namespace gamma_api;

table Attribute  {
  name:string;
  value:[ubyte];
}

table ResultItem {
  score:double;
  attributes:[Attribute];
  extra:string;
}

table SearchResult {
  total:int;
  result_code:vearch.status.Code;
  msg:string;
  result_items:[ResultItem];
}

table Response {
  results:[SearchResult];
  online_log_message:string;
}

root_type Response;
